{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "RateLimitQueueAPI": "Information"
    }
  },
  "AllowedHosts": "*",
  "Queue": {
    "MaxSize": 50,
    "TimeoutSeconds": 300,
    "ProcessingIntervalMs": 1000
  },
  "RateLimit": {
    "BaseRetryAfterSeconds": 1,
    "MaxRetryAfterSeconds": 60,
    "MaxRequestsPerMinute": 60
  }
}
